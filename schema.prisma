
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"

}

model User {
  id           String   @id @default(cuid())
  email        String   @unique
  passwordHash String
  name         String
  age          Int
  gender       String
  address      String
  interests    String[]
  createdAt    DateTime @default(now())
}
